#include <QCoreApplication>
#include "../sql/sqlite.h"
#include "handler.h"
#include "../tcp/tcp.h"

int main(int argc, char *argv[])
{
    QCoreApplication a(argc, argv);

    sBufType temp;
    sPrtcls Msg;

    cSqlite sql("employeeinfo.db");
    cHandler h(&sql,&Msg);

    cMyTcp MyTcp(11111);

    MyTcp.WaitClient();

    while(1)
    {
        MyTcp.RecvMsg(&Msg,sizeof(sPrtcls));
        h.ClientHandler();
        MyTcp.SendMsg(&Msg,sizeof(sPrtcls));
    }
    
    return a.exec();
}
